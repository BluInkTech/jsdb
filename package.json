	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@types/node": "22.1.0",
		"@vitest/coverage-v8": "^2.0.5",
		"memfs": "^4.11.1",
		"tsx": "4.17.0",
		"typescript": "5.5.4",
		"vitest": "^2.0.5"
	}
