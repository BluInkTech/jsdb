{
	"name": "jsdb",
	"version": "0.0.1",
	"description": "A fast NodeJS JSON database with reasonable scalability",
	"main": "index.js",
	"type": "module",
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"bench": "tsx --expose-gc",
		"build": "tsc",
		"dev": "tsx index.ts",
		"format": "biome format --write",
		"perf": "tsx --expose-gc test/operations.perf.ts",
		"test": "vitest run --coverage",
		"test-watch": "vitest",
		"type-check": "tsc --noEmit"
	},
	"author": "Seemant Rajvanshi",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/BluInkTech/jsdb"
	},
	"keywords": [
		"database",
		"json",
		"kv"
	],
	"devDependencies": {
		"@biomejs/biome": "1.8.3",
		"@types/node": "22.5.2",
		"@vitest/coverage-v8": "2.0.5",
		"tsx": "4.17.0",
		"typescript": "5.5.4",
		"vitest": "2.0.5"
	},
	"dependencies": {
		"roaring-wasm": "^1.0.1"
	}
}
